                var bolOrder = regOrder.test(Order);
                if (!bolOrder) {
                    alert("请输入订单号");
                    return false;
                }

                //手机
                var Dname = $("#txtDname").val().replace(/[ ]/g, "");
                var regDname = /^1[3-9][0-9]{9}$/;
                var bolDname = regDname.test(Dname);
                if (!bolDname) {
                    alert("请输入订货人手机");
                    return false;
                }

            });

            //提交
            $("#Button2").click(function () {

                //手机
                var Dhuo = $("#txtDhuo").val().replace(/[ ]/g, "");
                var regDhuo = /^1[3-9][0-9]{9}$/;
                var bolDhuo = regDhuo.test(Dhuo);
                if (!bolDhuo) {
                    alert("请输入订货人手机");
                    return false;
                }

                //手机
                var Shou = $("#txtShou").val().replace(/[ ]/g, "");
                var regShou = /^1[3-9][0-9]{9}$/;
                var bolShou = regShou.test(Shou);
                if (!bolShou) {
                    alert("请输入收货人手机");
                    return false;
                }

            });
        });
    </script>
